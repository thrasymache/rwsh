$.function_all_flags timings focus {
  .combine $focus (:	last\() ${.last_execution_time $focus} \) \	
  .combine (total\() ${.total_execution_time $focus} \) \	
  .combine (count\() ${.execution_count $focus} \) $nl}
$.function_all_flags clock-watch {
